{
  "name": "flappy-blink",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tensorflow/tfjs-core": "1.2.9",
    "@tensorflow/tfjs-node": "1.2.9",
    "face-api.js": "^0.20.0",
    "husky": "^2.3.0",
    "lint-staged": "^13.2.2",
    "node-sass": "^4.12.0",
    "pixi.js": "^5.0.3",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-pixi-fiber": "^0.8.0",
    "react-scripts": "3.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write --tab-width 4 --trailing-comma es5",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
